{#
/**
 * @file
 * Theme override to display the sidebar_first region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
<div class="meta">
  {#{ content }#}
  <a href="/user" class="portal-head__user" title="{{ 'Account'|t }}">
      <i class="fa fa-user" aria-hidden="true"></i>
  </a>
  <a href="{{ path('nodeshop.showCart') }}" class="portal-head__cart">
      <i class="fa fa-shopping-cart" aria-hidden="true"></i>
  </a>
  <a href="/search/bing" class="portal-head__search js-search">
      <i class="fa fa-search" aria-hidden="true"></i>
  </a>
  <a href="#" class="portal-head__hamburger js-hamburger" title="{{ 'open navigation'|t }}" aria-label="{{ 'open navigation'|t }}">
      <i class="fa fa-bars hamburger__icon" aria-hidden="true"></i>
  </a>
</div>
